<!--                 android:id="@+id/new_mouse_back" -->
<!--                 android:layout_width="0.0dip" -->
<!--                 android:layout_height="fill_parent" -->
<!--                 android:layout_weight="108.0" -->
<!--                 android:background="@drawable/new_mouse_back" /> -->

<!--             <ImageButton -->
<!--                 android:id="@+id/new_mouse_center" -->
<!--                 android:layout_width="0.0dip" -->
<!--                 android:layout_height="fill_parent" -->
<!--                 android:layout_weight="101.0" -->
<!--                 android:background="@drawable/new_mouse_center" /> -->

<!--             <ImageButton -->
<!--                 android:id="@+id/new_mouse_foward" -->
<!--                 android:layout_width="0.0dip" -->
<!--                 android:layout_height="fill_parent" -->
<!--                 android:layout_weight="111.0" -->
