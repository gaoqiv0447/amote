        <LinearLayout
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="fill_parent"
            android:layout_height="0.0dip"
            android:layout_weight="95.0"
            android:orientation="horizontal" >

            <ImageButton
                android:id="@+id/new_mouse_left"
                android:layout_width="0.0dip"
                android:layout_height="fill_parent"
                android:layout_weight="108.0"
                android:background="@drawable/new_mouse_left" />

            <ImageButton
                android:id="@+id/new_mouse_sensor"
                android:layout_width="0.0dip"
                android:layout_height="fill_parent"
                android:layout_weight="101.0"
                android:background="@drawable/new_mouse_sensor" />

            <ImageButton
                android:id="@+id/new_mouse_right"
                android:layout_width="0.0dip"
                android:layout_height="fill_parent"
                android:layout_weight="111.0"
                android:background="@drawable/new_mouse_right" />
        </LinearLayout>
    <LinearLayout
			xmlns:android="http://schemas.android.com/apk/res/android"
			android:layout_width="fill_parent"
			android:layout_height="0.0dip"
			android:layout_weight="60.0"
			android:background="#000000"
			android:orientation="horizontal" >

			<ImageButton android:id="@+id/home_switch_touchpad"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_weight="1"
				android:background="@drawable/back_home_switch" />

			<ImageButton android:id="@+id/main_control_switch_touchpad"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_weight="1"
				android:background="@drawable/main_control_switch" />

			<ImageButton android:id="@+id/touch_pad_switch_touchpad"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_weight="1"
				android:background="@drawable/touch_pad_switch" />

			<ImageButton android:id="@+id/game_control_switch_touchpad"
				android:layout_width="wrap_content" 
				android:layout_height="wrap_content"
				android:layout_weight="1"
				android:background="@drawable/game_control_switch" />

		</LinearLayout>		
    </LinearLayout>

